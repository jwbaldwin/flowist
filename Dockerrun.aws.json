{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "image": "jwbaldwin/flowist:latest",
      "name": "api",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "dev"
        }
      ]
    },
    {
      "image": "jwbaldwin/flowist-frontend:latest",
      "name": "frontend",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 80
        }
      ]
    }
  ],
  "family": "",
  "volumes": []
}
